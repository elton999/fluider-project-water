function on_message(self, message_id, message, sender)
	if message_id == hash("change_hair_color") then
		local color = message.hair_color_prop;
		local hair_color = vmath.vector4(color.x, color.y, color.z, 1)
		local body_part = gui.get_node("character_hair")
		gui.set_color(body_part, hair_color)
	end
end